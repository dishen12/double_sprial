<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="WPFInk.UpdateRecognitionRusult"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480" Width="331" Height="229">
	<Grid x:Name="LayoutRoot" Background="#FF675B5B" Margin="0" Height="229">
		<TextBox x:Name="_textBox" TextWrapping="Wrap" Margin="1,1,1,30" KeyDown="_textBox_KeyDown"/>
		<Button x:Name="OkButton" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="75" Content="Ok" Height="20" Margin="0,0,0,5" Click="OkButton_Click"/>
	</Grid>
</UserControl>
<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:WPFInk"
	x:Class="WPFInk.TitleInk"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="614">

	<Grid x:Name="LayoutRoot">
		<local:ScrollInkFrame x:Name="_scrollInkFrame" d:LayoutOverrides="Width, Height"/>
		<local:ControlPanel x:Name="Title_ControlPanel" Margin="8,-15,0,-138.05" d:LayoutOverrides="Width" VerticalAlignment="Top" HorizontalAlignment="Left" Visibility="Collapsed"/>
	</Grid>
</UserControl>